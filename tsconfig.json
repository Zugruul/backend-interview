{
	"compilerOptions": {
		"lib": ["es5", "es6", "es7", "esnext", "dom"],
		"module": "CommonJS",
		"target": "ES5",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"strict": true,
		"skipLibCheck": true,
		"noImplicitReturns": true,
		"isolatedModules": true,
		"useUnknownInCatchVariables": false,
		"outDir": "./lib",
		"declaration": true
	},
	"include": ["./src/**/*"]
}
